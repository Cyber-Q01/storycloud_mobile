{
  "expo": {
    "name": "StoryCloud",
    "slug": "StoryCloud",
    "owner": "fluxdev168",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "scheme": "storycloud",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.verilume113.StoryCloud"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
    
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
         
          "imageWidth": 200,
          "contentFit": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
      },
      "router": {}
    }
  }
}
